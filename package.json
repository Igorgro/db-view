    "devDependencies": {
        "electron": "^11.5.0",
        "electron-packager": "^14.2.1",
        "eslint": "^6.8.0"
    },
    "dependencies": {
        "eslint-plugin-json": "^2.1.1",
        "jquery": "^3.5.0",
        "pg": "^8.0.2"
    }
}
