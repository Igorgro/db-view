{
    "name": "db-viewer",
    "version": "1.0.0",
    "description": "Simple database viewer",
    "main": "index.js",
    "scripts": {
        "start": "npx electron .",
        "debug": "npx electron --inspect=5858 --remote-debugging-port=9222 .",
        "build-windows": "npx electron-packager ./ db-viewer --platform=win32 --arch=x64 --icon ./app/res/favicon.ico --out ./dist"
